<?xml version="1.0"?>
<package >
  <metadata>
    <id>TheMVCForum</id>
    <version>1.8.1</version>
    <title>The Official MVCForum</title>
    <authors>Aptitude</authors>
    <owners>Aptitude</owners>
    <projectUrl>http://www.mvcforum.com</projectUrl>
    <iconUrl>http://www.mvcforum.com/downloads/nuget-icon.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>MVCForum is a fully featured ASP.NET MVC 5 discussion board/forum. The software has some features similar to that seen on StackOverFlow plus LOTS more.</description>
    <summary>MVCForum is a fully featured ASP.NET MVC 5 discussion board/forum.</summary>
    <releaseNotes>See website for details about release</releaseNotes>
    <language>en-GB</language>
    <tags>forum</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.0">
        <dependency id="Akismet.NET" version="1.0" />
        <dependency id="WebActivator" version="1.5.2" />
        <dependency id="MarkdownSharp" version="1.13.0.0" />    
      </group>
      <group targetFramework=".NETFramework4.5">
        <dependency id="AntiXSS" version="4.3.0" />
        <dependency id="CommonServiceLocator" version="1.3" />
        <dependency id="EntityFramework" version="6.1.3" />
        <dependency id="ImageProcessor.Web" version="4.5.1" />
        <dependency id="jQuery" version="1.11.3" />
        <dependency id="jQuery.Validation" version="1.13.1" />
        <dependency id="Microsoft.AspNet.Mvc" version="5.2.3" />
        <dependency id="Microsoft.AspNet.WebApi" version="5.2.3" />
        <dependency id="Microsoft.Bcl.Build" version="1.0.21" />
        <dependency id="Microsoft.jQuery.Unobtrusive.Ajax" version="3.2.3" />
        <dependency id="Microsoft.jQuery.Unobtrusive.Validation" version="3.2.3" />
        <dependency id="Microsoft.AspNet.Web.Optimization" version="1.1.3" />
        <dependency id="Antlr" version="3.4.1.9004" />
        <dependency id="WebGrease" version="1.5.2" />
        <dependency id="Newtonsoft.Json" version="8.0.2" />
        <dependency id="Quartz" version="2.3.3" />      
        <dependency id="SquishIt.Mvc" version="0.9.8.2" />
        <dependency id="TinyMCE" version="4.3.3" />
        <dependency id="Unity" version="4.0.1" />
        <dependency id="HtmlAgilityPack" version="1.4.9" />
        <dependency id="EntityFramework.Cache" version="1.0.0" />
        <dependency id="Skybrud.Social" version="[0.9.4.1,1.0.0)" />
        <dependency id="EntityFramework.SqlServerCompact" version="6.1.3" />
        <dependency id="Common.Logging" version="3.3.1" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="lib\" target="lib" />
    <file src="tools\" target="tools" />
    <file src="content\" target="content" />
    <!-- Normal Files -->
    <file src="..\MVCForum.WebSite\App_Data\NuGetBackup\Hello.txt" target="content\App_Data\NuGetBackup" />
    <file src="..\MVCForum.WebSite\App_Data\forum.config" target="content\App_Data" />
    <file src="..\MVCForum.WebSite\Areas\Admin\Views\**" target="content\Areas\Admin\Views" />
    <file src="..\MVCForum.WebSite\Content\**" target="content\Content" exclude="..\MVCForum.WebSite\Content\uploads\**" />
    <file src="..\MVCForum.WebSite\Content\uploads\readme.txt" target="content\Content\uploads" />
    <file src="..\MVCForum.WebSite\Installer\**" target="content\Installer" />
    <file src="..\MVCForum.WebSite\Scripts\**" target="content\Scripts" exclude="..\MVCForum.WebSite\Scripts\tinymce\**"/>
    <file src="..\MVCForum.WebSite\Themes\**" target="content\Themes" />
    <file src="..\MVCForum.WebSite\Views\**" target="content\Views" />
    <file src="..\MVCForum.WebSite\web.config" target="content\web.config" />
    <file src="..\MVCForum.WebSite\Global.asax" target="content\Global.asax" />
    <!-- Compiled DLL's -->
    <file src="..\MVCForum.WebSite\bin\MVCForum.Website.dll" target="lib\MVCForum.Website.dll" />
    <file src="..\MVCForum.WebSite\bin\MVCForum.IOC.dll" target="lib\MVCForum.IOC.dll" />
    <file src="..\MVCForum.WebSite\bin\Badge.*.dll" target="lib" />
    <file src="..\MVCForum.WebSite\bin\MVCForum.Services.dll" target="lib\MVCForum.Services.dll" />
    <file src="..\MVCForum.WebSite\bin\MVCForum.Domain.dll" target="lib\MVCForum.Domain.dll" />
    <file src="..\MVCForum.WebSite\bin\MVCForum.Utilities.dll" target="lib\MVCForum.Utilities.dll" />
    <file src="..\MVCForum.WebSite\bin\Skybrud.Social.Microsoft.dll" target="lib\Skybrud.Social.Microsoft.dll" />
  </files>
</package>

