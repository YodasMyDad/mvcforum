@model MVCForum.Domain.DomainModel.General.Theme
@(
 Bundle.Css()
    .Add(Model.ThemePath + "/Content/Styles/imports.css")
    .Add("~/Content/slideoutpanel.css")
    .Add("~/Scripts/fancybox/jquery.fancybox.css")
    .Add("~/Content/bootstrap/css/bootstrap.css")
    .Add(Model.ThemePath + "/Content/Styles/Theme.min.css")
    .MvcRender("~/content/cache/combined_" + Model.ThemeName + "_#.css")
)