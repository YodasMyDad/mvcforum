@model List<MVCForum.Domain.DomainModel.Vote>
@{
    Layout = null;
}

<ul>
    @foreach (var vote in Model)
    {
        <li>
            <a href="@vote.VotedByMembershipUser.NiceUrl">
                <img src="@vote.VotedByMembershipUser.MemberImage(SiteConstants.Instance.GravatarLikedBySize)" alt="@vote.VotedByMembershipUser.UserName" title="@vote.VotedByMembershipUser.UserName">
            </a>
        </li>
    }
</ul>

